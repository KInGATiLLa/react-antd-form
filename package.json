    "react-input-mask": "^2.0.4",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^6.0.14",
    "remarkable": "^2.0.1",
    "styled-components": "^4.4.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "antd": "^4.5.1",
    "prop-types": "^15.7.2",
    "moment": "^2.27.0",
    "axios": "^0.19.2",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/preset-react": "^7.10.4",
