{
  "name": "react-antd-dynamic-form",
  "description": "Dynamic form built with ReactJS and Ant Design",
  "license": "MIT",
  "author": "KInGATiLLa",
  "version": "0.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/KInGATiLLa/react-antd-dynamic-form.git"
  },
  "homepage": "https://github.com/KInGATiLLa/react-antd-dynamic-form",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "build:lib": "NODE_ENV=production BABEL_ENV=production rollup -c",
    "build:watch": "rollup -c -w",
    "release": "npm publish"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "transform-react-remove-prop-types"
    ]
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
