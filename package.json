    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-less-modules": "^0.1.11",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
